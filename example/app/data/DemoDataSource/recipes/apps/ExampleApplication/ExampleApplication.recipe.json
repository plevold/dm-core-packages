    "plugin": "@development-framework/dm-core-plugins/header"
  },
  "uiRecipes": [
    {
      "name": "Explorer",
      "type": "CORE:UiRecipe",
      "plugin": "@development-framework/dm-core-plugins/explorer"
    },
    {
